<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".SuccessFragment">

    <Button
        android:id="@+id/button_readMore"
        style="@style/Widget.Material3.Button.IconButton.Outlined"
        android:layout_width="166dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:gravity="center_horizontal|center_vertical|end"
        android:paddingEnd="47dp"
        android:text="Read more"
        android:textAlignment="inherit"
        android:textAppearance="@style/MyTextStyle"
        android:textColor="@color/blue"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="@color/blue"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textView_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="154dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="154dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Your device is not secure!"
        android:textColor="@color/red"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.546"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:src="@drawable/ic_not_secure"
        app:layout_constraintBottom_toBottomOf="@+id/textView_success"
        app:layout_constraintStart_toEndOf="@+id/textView_success"
        app:layout_constraintTop_toTopOf="@+id/textView_success" />

    <!-- decorative_view_layout.xml -->


    <View
        android:id="@+id/decorativeView"
        android:layout_width="match_parent"
        android:layout_height="145dp"
        android:layout_margin="22dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/rounded_corners_edittext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2_success" />

    <TextView
        android:id="@+id/textView2_success"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="154dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="154dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Follow the link to read more"
        android:textColor="@color/red"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_success" />

    <TextView
        android:id="@+id/textView2_device"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Device"
        android:textColor="@color/red"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_device_r"
        app:layout_constraintStart_toStartOf="@+id/decorativeView"
        app:layout_constraintTop_toTopOf="@+id/decorativeView" />

    <TextView
        android:id="@+id/textView_type"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Type:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/textView2_device" />

    <TextView
        android:id="@+id/textView_typeInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_type"
        app:layout_constraintTop_toTopOf="@+id/textView2_device" />

    <TextView
        android:id="@+id/textView_brand"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Brand:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView_type" />

    <TextView
        android:id="@+id/textView_brandInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_brand"
        app:layout_constraintTop_toBottomOf="@+id/textView_typeInfo" />

    <TextView
        android:id="@+id/textView_model"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Model:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView_brand" />

    <TextView
        android:id="@+id/textView_modelInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_brand"
        app:layout_constraintTop_toBottomOf="@+id/textView_brandInfo" />

    <View
        android:id="@+id/decorativeView2"
        android:layout_width="match_parent"
        android:layout_height="145dp"
        android:layout_margin="22dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/rounded_corners_edittext"
        app:layout_constraintEnd_toEndOf="@+id/decorativeView"
        app:layout_constraintStart_toStartOf="@+id/decorativeView"
        app:layout_constraintTop_toBottomOf="@+id/decorativeView" />


    <TextView
        android:id="@+id/textView2_connectivity"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Connectivity"
        android:textColor="@color/red"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_connectivity"
        app:layout_constraintStart_toEndOf="@+id/imageView_connectivity"
        app:layout_constraintTop_toTopOf="@+id/imageView_connectivity" />


    <ImageView
        android:id="@+id/imageView_connectivity"
        android:layout_width="24dp"
        android:layout_height="13dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_connection_r"
        app:layout_constraintStart_toStartOf="@+id/decorativeView2"
        app:layout_constraintTop_toTopOf="@+id/decorativeView2" />


    <TextView
        android:id="@+id/textView_video"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Video:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/textView2_connectivity" />

    <TextView
        android:id="@+id/textView_videoInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_video"
        app:layout_constraintTop_toTopOf="@+id/textView2_connectivity" />

    <TextView
        android:id="@+id/textView_wifi"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="WIFI:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView_video" />
    <TextView
        android:id="@+id/textView_wifiInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_wifi"
        app:layout_constraintTop_toBottomOf="@+id/textView_videoInfo" />

    <TextView
        android:id="@+id/textView_support"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Support:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView_wifi" />

    <TextView
        android:id="@+id/textView_supportInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_support"
