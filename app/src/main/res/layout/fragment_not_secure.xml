    ///////////

    <View
        android:id="@+id/decorativeView3"
        android:layout_width="match_parent"
        android:layout_height="145dp"
        android:layout_margin="22dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/rounded_corners_edittext"
        app:layout_constraintEnd_toEndOf="@+id/decorativeView"
        app:layout_constraintStart_toStartOf="@+id/decorativeView2"
        app:layout_constraintTop_toBottomOf="@+id/decorativeView2" />


    <TextView
        android:id="@+id/textView2_security"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Security"
        android:textColor="@color/red"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_security"
        app:layout_constraintStart_toEndOf="@+id/imageView_security"
        app:layout_constraintTop_toTopOf="@+id/imageView_security" />


    <ImageView
        android:id="@+id/imageView_security"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_security_r"
        app:layout_constraintStart_toStartOf="@+id/decorativeView3"
        app:layout_constraintTop_toTopOf="@+id/decorativeView3" />


    <TextView
        android:id="@+id/textView_protocol"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Protocol:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_security"
        app:layout_constraintTop_toTopOf="@+id/textView2_security" />

    <TextView
        android:id="@+id/textView_protocolInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_protocol"
        app:layout_constraintTop_toTopOf="@+id/textView2_security" />

    <TextView
        android:id="@+id/textView_privacy"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Privacy shutter:"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_security"
        app:layout_constraintTop_toBottomOf="@+id/textView_protocol" />

    <TextView
        android:id="@+id/textView_privacyInfo"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/manrope_medium"
        android:gravity="center"
        android:text="Mobile"
        android:textColor="@color/gray"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/textView_privacy"
